package exceptions;

public abstract class AutoException extends Exception{
	
	static final long serialVersionUID = 1;
	
	private int errno;
	private String msg;
	
	public abstract void print();
	
	public int getErrno() {
		return errno;
	}

	public void setErrno(int errno) {
		this.errno = errno;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}
	
	
}
